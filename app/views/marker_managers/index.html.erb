<p id="notice"><%= notice %></p>

<h1>Listing Marker Managers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @marker_managers.each do |marker_manager| %>
      <tr>
        <td><%= link_to 'Show', marker_manager %></td>
        <td><%= link_to 'Edit', edit_marker_manager_path(marker_manager) %></td>
        <td><%= link_to 'Destroy', marker_manager, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Marker manager', new_marker_manager_path %>
